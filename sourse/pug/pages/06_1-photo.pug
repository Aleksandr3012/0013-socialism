extends layout/main.pug


include ../blocks/sContent/_sContent.pug
include ../blocks/sContact/_sContact.pug 
include ../blocks/sAction/_sAction.pug 
include ../blocks/sNumberParticipants/_sNumberParticipants.pug 
include ../blocks/sSocial/_sSocial.pug 
include ../blocks/sAside/_sAside.pug 
include ../blocks/sEvents/_sEvents.pug 
include ../blocks/sCalendar/_sCalendar.pug 

block variables
	- let  mainPage= 'mainPage'
block title
	- let  title= 'Фотогалерея'
	title= title
block pagecontent 
	+header()
		+top-nav()
	+pageHead('Главная страница', 'фотогалерея', '22 МАРТА 2020Г. ОДИНОЧНЫЕ ПИКЕТЫ ТОМСКИХ АКТИВИСТОВ ОБЩЕСТВЕННОГО ДВИЖЕНИЯ "ЗА НОВЫЙ СОЦИАЛИЗМ!"')
		+pic('flag-2.jpg', 'bg', 'lazy')(class="picture-bg")
		h1 22 марта 2020г. Одиночные пикеты томских активистов общественного движения "За Новый Социализм!"
	.containerLimited
		.container.contentWrap
			.row
				.col-md-8
					+mainBlock()
						+e.photoItem--inner
							+e.title 22 марта 2020г. Одиночные пикеты томских активистов общественного движения "За Новый Социализм!"
								+e.date 22.03.2020
							+e.photo
								+e.img-wrap
									a(href=`img/@2x/photo-1-1.jpg` data-fancybox=`photo-1`)
										img.lazy(src="#" data-src=`img/@2x/photo-1-1.jpg` alt="")
								+e.img-wrap
									a(href=`img/@2x/photo-1-2.jpg` data-fancybox=`photo-1`)
										img.lazy(src="#" data-src=`img/@2x/photo-1-2.jpg` alt="")
								+e.img-wrap
									a(href=`img/@2x/photo-1-3.jpg` data-fancybox=`photo-1`)
										img.lazy(src="#" data-src=`img/@2x/photo-1-3.jpg` alt="")
								+e.img-wrap
									a(href=`img/@2x/photo-1-4.jpg` data-fancybox=`photo-1`)
										img.lazy(src="#" data-src=`img/@2x/photo-1-4.jpg` alt="")

				.col-md-4
					//- +sAsideMenuVideo()
					+sAside()






	
	+sCalendar()
	+footer()